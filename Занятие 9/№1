import random


def g(n):
    per = 0
    s = 0
    aar = []
    a = [[random.randrange(10) for _ in range(n)] for _ in range(n)]
    for row in range(n):
        for col in range(row + 1, n):
            s += a[row][col]
    for row in range(n):
        aar.append(input())
        if n > 0:
            per += 1
    print('Сумма:', s)
    print('Число:', per)
